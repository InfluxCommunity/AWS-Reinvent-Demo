        {
          "datasource": {
            "type": "influxdata-flightsql-datasource",
            "uid": "${DS_SERVERLESS}"
          },
          "format": "table",
          "queryText": "WITH transitions AS (\n  SELECT \n    t._time, \n    CASE\n      WHEN t.amperage >= 0.02 THEN 1\n      ELSE 0\n    END as current_state,\n    LEAD(\n      CASE\n        WHEN t.amperage >= 0.02 THEN 1\n        ELSE 0\n      END, 1, 0\n    ) OVER (ORDER BY t._time) as next_state\n  FROM (\n    SELECT\n      DATE_BIN(INTERVAL '20 seconds', time, TIMESTAMP '2022-01-01 00:00:00Z') AS _time,\n      (ABS(median(\"pin_voltage\") / 3.3) * 100) as amperage \n    FROM \"power_meter\"\n    WHERE $__timeRange(time)\n    GROUP BY _time\n  ) as t\n)\n\nSELECT COUNT(*) as coffee_count\nFROM transitions\nWHERE current_state = 1 AND next_state = 0;",
          "rawEditor": true,
          "rawQuery": true,
          "refId": "A"
        }
      ],
      "title": "Number of Coffees",
      "transformations": [],
      "type": "bargauge"
