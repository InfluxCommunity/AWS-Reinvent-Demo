          "datasource": {
            "type": "influxdata-flightsql-datasource",
            "uid": "${DS_SERVERLESS}"
          },
          "format": "table",
          "queryText": "SELECT \n  t._time, \n  CASE\n    WHEN t.amperage >= 0.02 THEN 'brewing'\n    ELSE 'standby'\n  END as state\nFROM (\n  SELECT\n    DATE_BIN(INTERVAL '20 seconds', time, TIMESTAMP '2022-01-01 00:00:00Z') AS _time,\n    (ABS(median(\"pin_voltage\") / 3.3) * 100) as amperage \n  FROM \"power_meter\"\n  WHERE $__timeRange(time)\n  GROUP BY _time\n) as t\nORDER BY t._time",
          "rawEditor": true,
